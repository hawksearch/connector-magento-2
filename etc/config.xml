<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <hawksearch_connector>
            <api_settings>
                <mode>develop</mode>
                <hawk_url>
                    <develop>https://dev.hawksearch.net/</develop>
                    <staging>https://test.hawksearch.net/</staging>
