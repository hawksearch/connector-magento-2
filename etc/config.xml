                </tracking_url>

                <rec_url>
                    <develop>https://recs-dev.hawksearch.net/</develop>
                    <staging>https://recs-test.hawksearch.net/</staging>
                    <production>https://recs.hawksearch.com/</production>
                </rec_url>
            </api_settings>
        </hawksearch_connector>
    </default>
</config>
